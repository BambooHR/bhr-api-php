{
    {{#composerPackageName}}
    "name": "{{{.}}}",
    {{/composerPackageName}}
    {{#artifactVersion}}
    "version": "{{{.}}}",
    {{/artifactVersion}}
    "description": "{{{appDescription}}}",
    "repositories": [
        {
            "type": "composer",
            "url": "https://REMOVED"
        }
    ],
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "{{{artifactUrl}}}",
    "license": "{{{licenseName}}}",
    "authors": [
        {
            "name": "{{{developerOrganization}}}",
            "homepage": "{{{developerOrganizationUrl}}}"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0",
        "mustache/mustache": "^2.14"
    },
    "require-dev": {
        "bamboohr/phpcs": "^1.0.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "post-install-cmd": [
            "./vendor/bin/phpcs --config-set installed_paths ${PWD}/vendor/bamboohr/phpcs"
        ],
        "post-update-cmd": [
            "./vendor/bin/phpcs --config-set installed_paths ${PWD}/vendor/bamboohr/phpcs"
        ]
    },
    "autoload": {
        "psr-4": { "{{{escapedInvokerPackage}}}\\" : "{{{srcBasePath}}}/" }
    },
    "autoload-dev": {
        "psr-4": { "{{{escapedInvokerPackage}}}\\Test\\" : "{{{testBasePath}}}/" }
    }
}
